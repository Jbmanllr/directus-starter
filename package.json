  "license": "ISC",
  "dependencies": {
    "@pixielabs/directus-extension-blurhash": "^1.0.0",
    "directus-extension-computed-interface": "^1.9.0",
    "directus-extension-display-link": "^1.6.1",
    "directus-extension-group-modal-interface": "^1.2.0",
    "directus-extension-group-tabs-interface": "^1.2.2",
    "directus-extension-inline-form-interface": "^1.1.0",
    "directus-extension-tags-m2m-interface": "^0.8.0",
    "directus-extension-generate-types": "^0.5.1",
    "directus-extension-schema-sync": "*"
  }
}
