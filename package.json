{
  "name": "directus-starter",
  "version": "1.0.0",
  "description": "My starter template for Directus.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "./update.sh",
    "build": "docker build -t directus-starter:latest .",
    "snap": "npx directus schema snapshot ./snapshot.yaml",
    "date-snap": "npx directus schema snapshot ./snapshots/\"$(date \"+%F\")\"-snapshot-\"$(date \"+%s\")\".yaml",
    "dev": "docker compose up"
  },
